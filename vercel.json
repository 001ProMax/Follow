{
  "rewrites": [
    {
      "source": "/__debug_proxy",
      "destination": "/__debug_proxy.html"
    },
    {
      "source": "/__debug_proxy/:path*",
      "destination": "/__debug_proxy.html"
    }
  ],
  "routes": [
    {
      "src": "/[^.]+",
      "dest": "/",
      "status": 200
    }
  ]
}
